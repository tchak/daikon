{
  "private": true,
  "name": "daikon",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:start": "node -r dotenv/config node_modules/.bin/remix dev",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:clean": "del .cache ./build public/build app/styles",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "build:graphql": "graphql-codegen -r dotenv/config --config codegen.yml",
    "build:remix": "remix build",
    "build:size": "size-limit",
    "lint": "run-s lint:*",
    "lint:types": "tsc",
    "lint:js": "eslint . --ext .ts,.tsx",
    "postinstall": "remix setup",
    "start": "remix-serve ./build"
  },
  "dependencies": {
    "@envelop/core": "^1.7.1",
    "@envelop/depth-limit": "^1.2.1",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@popperjs/core": "^2.11.0",
    "@prisma/client": "^3.8.1",
    "@reach/auto-id": "^0.16.0",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@tanem/react-nprogress": "^4.0.2",
    "@urql/core": "^2.3.6",
    "@urql/exchange-execute": "^1.2.2",
    "chalk": "^4.0.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "graphql": "^16.2.0",
    "graphql-helix": "^1.11.0",
    "graphql-scalars": "^1.14.1",
    "immer": "^9.0.12",
    "is-hotkey": "^0.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-popper": "^2.2.5",
    "react-table": "^7.7.0",
    "react-use-clipboard": "^1.0.7",
    "remix": "^1.0.6",
    "remix-auth": "^3.2.1",
    "remix-auth-form": "^1.1.1",
    "remix-utils": "^2.4.0",
    "size-limit": "^7.0.4",
    "tiny-invariant": "^1.2.0",
    "truncate": "^3.0.0",
    "uuid": "^8.3.2",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typed-document-node": "^2.2.1",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@remix-run/dev": "^1.0.6",
    "@size-limit/preset-app": "^7.0.4",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/is-hotkey": "^0.1.6",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.10",
    "@types/react-table": "^7.7.9",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "cross-env": "^7.0.3",
    "del-cli": "^4.0.1",
    "dotenv": "^11.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-typestrict": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
