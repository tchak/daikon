    "@remix-run/react": "^0.19.2",
    "@remix-run/serve": "^0.19.2",
    "@tanem/react-nprogress": "^3.0.67",
    "@urql/core": "^2.3.3",
    "clsx": "^1.1.1",
    "dotenv": "^10.0.0",
    "graphql": "^15.6.1",
    "is-hotkey": "^0.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-popper": "^2.2.5",
    "react-router-dom": "6.0.0-beta.6",
    "react-table": "^7.7.0",
    "react-use-clipboard": "^1.0.7",
    "remix": "^0.19.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
