    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-table": "^7.7.6",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-cli": "^9.0.1",
    "tailwindcss": "^2.1.4",
    "typescript": "^4.1.2"
